{
  "system_info": {
    "name": "Ultimate Image Automation System MVP",
    "version": "1.0.0",
    "created": "2025-08-31",
    "description": "기존 CUA-MASTER, AI Council, Cafe24와 완전 통합된 AI 이미지 생성 자동화 시스템"
  },
  "api_configuration": {
    "openai": {
      "enabled": true,
      "models": [
        "gpt-4",
        "dalle-3"
      ],
      "endpoint": "https://api.openai.com/v1",
      "rate_limits": {
        "requests_per_minute": 60,
        "tokens_per_minute": 90000
      }
    },
    "anthropic": {
      "enabled": true,
      "models": [
        "claude-3-sonnet",
        "claude-3-opus"
      ],
      "endpoint": "https://api.anthropic.com/v1",
      "rate_limits": {
        "requests_per_minute": 50,
        "tokens_per_minute": 80000
      }
    },
    "google": {
      "enabled": true,
      "models": [
        "gemini-2.0-flash-thinking"
      ],
      "endpoint": "https://generativelanguage.googleapis.com/v1",
      "rate_limits": {
        "requests_per_minute": 60,
        "tokens_per_minute": 120000
      }
    },
    "perplexity": {
      "enabled": true,
      "models": [
        "pplx-7b-online",
        "pplx-70b-online"
      ],
      "endpoint": "https://api.perplexity.ai",
      "rate_limits": {
        "requests_per_minute": 40,
        "tokens_per_minute": 60000
      }
    }
  },
  "image_generation": {
    "default_model": "dalle3",
    "supported_models": {
      "dalle3": {
        "provider": "openai",
        "max_size": "1792x1024",
        "styles": [
          "vivid",
          "natural"
        ],
        "cost_per_image": 0.04
      },
      "stable_diffusion": {
        "provider": "local",
        "max_size": "1536x1536",
        "styles": [
          "realistic",
          "artistic",
          "anime",
          "3d"
        ],
        "cost_per_image": 0.001
      }
    },
    "quality_settings": {
      "high": {
        "steps": 50,
        "guidance_scale": 7.5,
        "resolution": "1024x1024"
      },
      "standard": {
        "steps": 30,
        "guidance_scale": 7.0,
        "resolution": "512x512"
      },
      "fast": {
        "steps": 20,
        "guidance_scale": 6.0,
        "resolution": "512x512"
      }
    }
  },
  "prompt_templates": {
    "ecommerce": {
      "product_detail": {
        "base": "Professional product photography for e-commerce",
        "modifiers": {
          "clean": "white background, studio lighting, minimal composition",
          "lifestyle": "natural setting, ambient lighting, contextual usage",
          "premium": "luxury presentation, dramatic lighting, high-end appeal"
        },
        "negative_prompts": "low quality, blurry, distorted, watermark, text"
      },
      "marketing_banner": {
        "base": "Marketing banner design for digital advertising",
        "modifiers": {
          "sale": "promotional design, attention-grabbing, discount emphasis",
          "brand": "brand-focused, consistent identity, professional look",
          "seasonal": "seasonal theme, timely relevance, festive elements"
        },
        "negative_prompts": "cluttered, illegible text, poor composition"
      }
    },
    "social_media": {
      "instagram_post": {
        "base": "Instagram-optimized visual content",
        "modifiers": {
          "story": "9:16 aspect ratio, mobile-friendly, engaging",
          "feed": "1:1 aspect ratio, aesthetic appeal, shareable",
          "reel": "vertical format, dynamic composition, eye-catching"
        },
        "negative_prompts": "horizontal format, poor mobile display, text-heavy"
      }
    }
  },
  "ai_council_integration": {
    "enabled": true,
    "models_for_analysis": [
      "gpt-4",
      "claude-3-sonnet",
      "gemini-2.0-flash"
    ],
    "consensus_threshold": 0.7,
    "analysis_criteria": {
      "composition": 0.25,
      "technical_quality": 0.25,
      "brand_consistency": 0.25,
      "commercial_viability": 0.25
    }
  },
  "cafe24_integration": {
    "enabled": true,
    "auto_upload": false,
    "image_formats": [
      "jpg",
      "png",
      "webp"
    ],
    "size_variants": {
      "thumbnail": "150x150",
      "medium": "300x300",
      "large": "800x800",
      "original": "1200x1200"
    },
    "watermark": {
      "enabled": false,
      "position": "bottom-right",
      "opacity": 0.3
    }
  },
  "processing_settings": {
    "max_concurrent_tasks": 3,
    "queue_size_limit": 100,
    "retry_attempts": 3,
    "timeout_seconds": 120,
    "cache_duration_hours": 24
  },
  "output_configuration": {
    "base_directory": "C:/Users/8899y/CUA-MASTER/output/images",
    "subdirectories": {
      "generated": "generated",
      "processed": "processed",
      "thumbnails": "thumbnails",
      "metadata": "metadata"
    },
    "filename_format": "{timestamp}_{model}_{hash}",
    "metadata_format": "json"
  },
  "monitoring": {
    "logging_level": "INFO",
    "log_file": "C:/Users/8899y/CUA-MASTER/logs/image_automation.log",
    "metrics_collection": true,
    "performance_tracking": true,
    "error_reporting": true
  },
  "scheduling": {
    "enabled": true,
    "default_timezone": "Asia/Seoul",
    "batch_processing_hours": [
      "02:00",
      "14:00",
      "22:00"
    ],
    "maintenance_window": "03:00-04:00"
  },
  "security": {
    "api_key_encryption": true,
    "rate_limiting": true,
    "request_validation": true,
    "output_sanitization": true
  },
  "experimental_features": {
    "style_transfer": false,
    "background_removal": true,
    "image_upscaling": true,
    "batch_optimization": true,
    "ai_feedback_loop": true
  }
}